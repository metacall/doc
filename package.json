{
  "name": "metacall-tutorial",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "dev": "docusaurus dev",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.mdx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.mdx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "prepare": "husky"
  },
  "dependencies": {
    "@docusaurus/core": "3.7.0",
    "@docusaurus/preset-classic": "3.7.0",
    "@docusaurus/theme-search-algolia": "^3.7.0",
    "@mantine/carousel": "^7.17.0",
    "@mantine/core": "^7.17.0",
    "@mdx-js/react": "^3.0.0",
    "@tabler/icons-react": "^3.30.0",
    "clsx": "^2.0.0",
    "metacall": "^0.4.3",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-xarrows": "^2.0.2"
  },
  "devDependencies": {
    "@docusaurus/eslint-plugin": "^3.9.2",
    "@docusaurus/module-type-aliases": "3.7.0",
    "@docusaurus/types": "3.7.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-mdx": "^3.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "husky": "^9.1.7",
    "prettier": "^3.2.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{md,mdx,json}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
